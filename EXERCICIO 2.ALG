Algoritmo "exercicio 2"

Var
   num: inteiro
   num2: inteiro


Inicio
   //seção de comandos
   escreval ("Digite o primeiro número")
   leia (num)
   escreval ("Digite o segundo número")
   leia (num2)

   se (num>num2) entao
      escreva ("Número 1 é maior que o número 2")
      fimse

    se (num2>num) entao
         escreva ("Número 2 é maior do que o número 1")
      fimse


Fimalgoritmo